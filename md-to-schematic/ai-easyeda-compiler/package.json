{
  "name": "ai-easyeda-compiler",
  "version": "2.0.0",
  "description": "Production-grade AI-powered EasyEDA schematic compiler with full integration",
  "main": "compiler-mvp.js",
  "scripts": {
    "run": "node cli.js",
    "start": "node cli.js",
    "compile:watch": "nodemon compiler-mvp.js",
    "quick-start": "node cli.js compile && node cli.js integrate",
    "full-pipeline": "node cli.js compile && node cli.js convert",
    "auto:fast": "node auto-pcb-designer-optimized.js",
    "auto": "node auto-pcb-designer.js",
    "harvest": "node knowledge-harvester.js",
    "learn": "node learning-engine.js learn",
    "evaluate": "node learning-engine.js evaluate",
    "trend": "node learning-engine.js trend",
    "report": "node learning-engine.js report",
    "status": "node system-check.js",
    "compile": "node compiler-mvp.js",
    "integrate": "node integrate.js",
    "convert": "node convert-to-easyeda.js",
    "download": "node datasheet-downloader.js",
    "knowledge": "node component-knowledge-loader.js list",
    "check": "node -e \"console.log('Checking Ollama...'); require('child_process').exec('ollama list', (e,o) => console.log(o || 'Ollama not found'))\"",
    "smart-learn": "node smart-learning-system.js",
    "process-vmc": "node process-vmc-rules.js"
  },
  "keywords": [
    "easyeda",
    "pcb",
    "ai",
    "ollama",
    "electronics",
    "schematic",
    "compiler",
    "integration",
    "automation"
  ],
