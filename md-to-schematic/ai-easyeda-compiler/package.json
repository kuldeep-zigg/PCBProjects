    "markdown-it": "^14.0.0",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.36.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
